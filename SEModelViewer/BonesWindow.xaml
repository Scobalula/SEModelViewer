<Window x:Class="SEModelViewer.BonesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SEModelViewer"
        mc:Ignorable="d"
        Title="Bones" Height="419" Width="350" WindowStartupLocation="CenterOwner">
    <Grid>
        <ListView x:Name="Bones" Background="#FF1F1F1F" Foreground="White" BorderBrush="#FF1D1D1D">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Name" Width="128" DisplayMemberBinding="{Binding Name}"/>
                    <GridViewColumn Header="Position" Width="192" DisplayMemberBinding="{Binding Position}"/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
